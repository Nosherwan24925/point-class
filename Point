#include<iostream>
using namespace std;

class Point
{
private:
long area;
long volume;

public:
long Area(long length,long width);
long Volume(long length,long width,long bredth);

}

long Point::Volume(long longth,long width,long bredth)
{

volume=length*width*bredth;
return volume;

}

long Point::Area(long length,long width)
{
	area=length*width;
	return area;

}

